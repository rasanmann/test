{
  "name": "yqb/drupal",
  "description": "Aéroport international Jean-Lesage de Québec (YQB)",
  "type": "project",
  "license": "proprietary",
  "config": {
    "sort-packages": true
  },
  "require": {
    "composer/installers": "^1.0.24",
    "cweagans/composer-patches": "^1.6.0",
    "drupal-composer/drupal-scaffold": "^2.0.0",
    "drupal/addtoany": "~1.12",
    "drupal/ape": "^1.4",
    "drupal/bootstrap": "~3.5",
    "drupal/bootstrap_layouts": "^5.1",
    "drupal/composer_manager": "1.0-rc2",
    "drupal/config_update": "~1.3",
    "drupal/console": "~1.0",
    "drupal/core": "~8.7.11",
    "drupal/ctools": "3.2",
    "drupal/devel": "~2.1",
    "drupal/editor_advanced_link": "1.6",
    "drupal/editor_file": "1.4",
    "drupal/email_registration": "1.0-rc5",
    "drupal/entity_reference_revisions": "~1.3",
    "drupal/field_collection": "1.0-alpha1",
    "drupal/image_url_formatter": "1.0-beta1",
    "drupal/insert_block": "1.x-dev",
    "drupal/layout_plugin": "^1.0@alpha",
    "drupal/mailsystem": "4.2",
    "drupal/memcache": "2.0",
    "drupal/menu_link_attributes": "~1.0",
    "drupal/metatag": "~1.7",
    "drupal/migrate_plus": "4.2.0",
    "drupal/migrate_source_csv": "~2.0",
    "drupal/migrate_tools": "4.5.0",
    "drupal/page_manager": "4.0-beta4",
    "drupal/panels": "^4.0",
    "drupal/paragraphs": "~1.1",
    "drupal/pathauto": "~1.0",
    "drupal/redirect": "~1.0",
    "drupal/restui": "1.17",
    "drupal/scheduler": "^1.0",
    "drupal/search_autocomplete": "1.1",
    "drupal/stage_file_proxy": "^1.0@alpha",
    "drupal/swiftmailer": "~1.0",
    "drupal/token": "~1.0",
    "drupal/views_field_view": "1.0-beta2",
    "drupal/webform": "~5.4",
    "drupal/yamlform": "1.0-beta29",
    "drush/drush": "^9.3",
    "league/csv": "^8.0",
    "wikimedia/composer-merge-plugin": "^1.0"
  },
  "require-dev": {
    "drupal/config_devel": "1.0-rc1+8-dev",
    "drupal/kint": "~2.1"
  },
  "conflict": {
    "drupal/core": "7.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/libraries/{$name}": [
        "type:drupal-library"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ]
    },
    "enable-patching": true,
    "patches": {
      "drupal/views_field_view": {
        "Views field view PHP warnings": "patches/views_field_view.patch"
      },
      "drupal/core": {
        "Multiple usages of FieldPluginBase::getEntity do not check for NULL, leading to WSOD": "https://www.drupal.org/files/issues/2019-11-12/3007424-57-8-7-x.patch"
      }
    },
    "merge-plugin": {
      "include": [
        "docroot/modules/custom/*/composer.json",
        "docroot/themes/custom/*/composer.json"
      ],
      "recurse": true
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@composer drupal-scaffold",
      "find docroot vendor -name '.git' | xargs rm -rf"
    ],
    "post-update-cmd": [
      "@composer drupal-scaffold",
      "find docroot vendor -name '.git' | xargs rm -rf"
    ],
    "drupal-scaffold": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
