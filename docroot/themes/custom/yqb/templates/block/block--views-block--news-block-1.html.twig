{#
/**
 * @file
 * Default theme implementation to display a block.
 */
#}
<section{{ attributes.addClass('block', 'clearfix') }}>
  {{ title_prefix }}
  {% if label %}
    <h2{{ title_attributes.addClass('block-title', 'text-center', 'title-section') }}>{{ label }}</h2>
  {% endif %}
  {{ title_suffix }}

  {% block content %}
    {{ content }}
  {% endblock %}
</section>

