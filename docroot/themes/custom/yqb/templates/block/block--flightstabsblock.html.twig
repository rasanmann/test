{#
/**
 * @file
 * Default theme implementation to display a block.
 * @ingroup templates
 */
#}
<div class="row-inner row-cols-gutter">
		<div class="col-xs-12">
		<section{{ attributes.addClass('block', 'clearfix') }}>
		  {{ title_prefix }}
		  {% if label %}
			<h2{{ title_attributes.addClass('block-title') }}>{{ label }}</h2>
		  {% endif %}
		  {{ title_suffix }}
		
		  {% block content %}
			{{ content }}
		  {% endblock %}
		</section>
	</div>
</div>

